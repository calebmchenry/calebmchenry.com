{
    "files.autoSave": "off",
	"editor.formatOnSave": false,
	"prettier.disableLanguages": [
		"javascript",
		"javascriptreact",
		"typescript",
		"typescriptreact"
	],
	"eslint.validate": [
		"javascript",
		"javascriptreact",
		"typescript",
		"typescriptreact",
	],
	"editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
	}
}